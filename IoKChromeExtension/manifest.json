{
  "manifest_version": 2,
  "name": "Junction19 Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "js": ["jquery-3.4.1.min.js", "./components/fb_fake_request/fb_fake_request.js"],
      "css": ["./components/fb_fake_request/fb_fake_request.css"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "*.html",
    "*.css",
    "*.js",
    "components/fb_fake_request/pics/*"
  ]
}
