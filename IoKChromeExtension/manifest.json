{
  "manifest_version": 2,
  "name": "Junction19 Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
  "webRequest",
    "webRequestBlocking",
  "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
